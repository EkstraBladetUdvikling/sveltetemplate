    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-svg": "^1.0.0-beta.6",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.2",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@ekstra-bladet/designsystem": "7.1.0",
    "sirv-cli": "^2.0.2"
