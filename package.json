    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-svelte-svg": "^1.0.0-beta.6",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@ekstra-bladet/designsystem": "7.7.1",
    "sirv-cli": "^2.0.2"
